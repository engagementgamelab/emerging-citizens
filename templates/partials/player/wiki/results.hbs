{{!-- PLAYER RESULTS --}}
<section id="results">
	{{#if last_round}}

        <div class="typeform-widget" data-url="https://jordan243.typeform.com/to/FvLo7c" data-text="WikiGeeks Feedback" style="width:100%;height:500px;"></div>
        <script>(function(){var qs,js,q,s,d=document,gi=d.getElementById,ce=d.createElement,gt=d.getElementsByTagName,id='typef_orm',b='https://s3-eu-west-1.amazonaws.com/share.typeform.com/';if(!gi.call(d,id)){js=ce.call(d,'script');js.id=id;js.src=b+'widget.js';q=gt.call(d,'script')[0];q.parentNode.insertBefore(js,q)}})()</script>
        {{!-- <div style="font-family: Sans-Serif;font-size: 12px;color: #999;opacity: 0.5; padding-top: 5px;">Powered by <a href="https://www.typeform.com/examples/forms/?utm_campaign=ptDwLc&amp;utm_source=typeform.com-3552517-Basic&amp;utm_medium=typeform&amp;utm_content=typeform-embedded-poweredbytypeform&amp;utm_term=EN" style="color: #999" target="_blank">Typeform</a></div> --}}

  {{else}}

		<div class="row">
			
			<div class="col-xs-12 col-sm-12">

							
					<div class="row">
		
						<div class="col-xs-6 col-sm-6">
							<div class="content" id="clicks">

								<h2>clicks</h2>
								<span>{{clicks}}</span>

							</div>
						</div>

						<div class="col-xs-6 col-sm-6">
							<div class="content" id="time">

								<h2>time</h2>
								<span>{{time}}</span>
							
							</div>
						</div>

					</div>

					<div class="row">

						<div id="articles" class="col-lg-6 col-centered">
						{{jsonPrint this}}

							<h2 id="player-name">{{username}}:</h2>

							{{#each articles}}

								<div class="row">

									{{!-- First article --}}
									{{#if @first}}
									
										<h3 id="firstArticle" class="articleText">{{this}}</h3>
									
									{{else}}

										<div class="articleGroup">

											<svg height="70" width="50">
													
												<line class="articleLine" x1="25" y1="10" x2="25" y2="0" style="stroke:rgb(188,255,252);stroke-width:8"></line>

												<circle class="articleDot" cx="25" cy="10" r="0" stroke="black" stroke-width="0" fill="red"></circle>
												<circle class="articleDot" cx="25" cy="55" r="0" stroke="black" stroke-width="0" fill="red"></circle>

											</svg>
								
											<div class="row articleText">

												{{!-- Final article (if player finished) --}}
												{{#if @last}}
													{{#if ../finished}}
													
														<h3>{{this}}</h3>
													
													{{else}}
													
														{{this}}
													
													{{/if}}

												{{else}}

													{{this}}

												{{/if}}

											</div>

										</div>
									
									{{/if}}

								</div>

							{{/each}}

						</div>

					</div>
				
			</div>

		</div>

	{{/if}}

</section>